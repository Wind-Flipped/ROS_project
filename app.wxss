@import '/static/iconfont/iconfont.wxss';
page {
    width: 750rpx;
    background-color: #f5f5f5;
}
.page-container {
    width: 100%;
    height: 100vh;
}

.width-100vw {
    width: 100%;
}

.flex-row {
    display: flex;
    flex-direction: row;
}

.flex-col {
    display: flex;
    flex-direction: column;
}

.align-center {
    align-items: center;
}

.justify-center {
    justify-content: center;
}

.justify-between {
    justify-content: space-between;
}

.margin-top-30 {
    margin-top: 30rpx;
}

.margin-top-25 {
    margin-top: 25rpx;
}

